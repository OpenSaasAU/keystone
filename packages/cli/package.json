{
  "name": "@opensaas/keystone-cli",
  "version": "0.0.3",
  "repository": "https://github.com/keystonejs/keystone/tree/main/packages/cli",
  "license": "MIT",
  "main": "dist/keystone-6-cli.cjs.js",
  "module": "dist/keystone-6-cli.esm.js",
  "exports": {
    "./scripts/cli": {
      "module": "./scripts/cli/dist/opensaas-keystone-cli-scripts-cli.esm.js",
      "default": "./scripts/cli/dist/opensaas-keystone-cli-scripts-cli.cjs.js"
    },
    "./scripts": {
      "module": "./scripts/dist/opensaas-keystone-cli-scripts.esm.js",
      "default": "./scripts/dist/opensaas-keystone-cli-scripts.cjs.js"
    },
    "./package.json": "./package.json",
    "./bin/cli.js": "./bin/cli.js"
  },
  "bin": {
    "keystone": "bin/cli.js"
  },
  "dependencies": {
    "@emotion/hash": "^0.9.2",
    "@nodelib/fs.walk": "^2.0.0",
    "@prisma/internals": "5.21.1",
    "@prisma/migrate": "5.21.1",
    "chalk": "^4.1.2",
    "conf": "^10.2.0",
    "esbuild": "^0.24.0",
    "fs-extra": "^11.2.0",
    "meow": "^9.0.0",
    "prisma": "5.21.1",
    "prompts": "^2.4.2",
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@opensaas/keystone-core": "workspace:^",
    "@opensaas/keystone-admin-ui": "workspace:^",
    "@types/fs-extra": "^11.0.0",
    "@types/prompts": "^2.0.14",
    "@types/resolve": "^1.20.2"
  },
  "peerDependencies": {
    "@opensaas/keystone-core": "workspace:^"
  },
  "preconstruct": {
    "entrypoints": [
      "scripts/index.ts",
      "scripts/cli.ts"
    ],
    "exports": {
      "extra": {
        "./bin/cli.js": "./bin/cli.js"
      }
    }
  }
}
