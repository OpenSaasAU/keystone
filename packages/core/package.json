{
  "name": "@opensaas/keystone-core",
  "version": "0.0.4",
  "repository": "https://github.com/opensaas/keystone/tree/main/packages/core",
  "license": "MIT",
  "main": "dist/opensaas-keystone-core.cjs.js",
  "module": "dist/opensaas-keystone-core.esm.js",
  "exports": {
    ".": {
      "module": "./dist/opensaas-keystone-core.esm.js",
      "default": "./dist/opensaas-keystone-core.cjs.js"
    },
    "./access": {
      "module": "./access/dist/opensaas-keystone-core-access.esm.js",
      "default": "./access/dist/opensaas-keystone-core-access.cjs.js"
    },
    "./context": {
      "module": "./context/dist/opensaas-keystone-core-context.esm.js",
      "default": "./context/dist/opensaas-keystone-core-context.cjs.js"
    },
    "./lib": {
      "module": "./lib/dist/opensaas-keystone-core-lib.esm.js",
      "default": "./lib/dist/opensaas-keystone-core-lib.cjs.js"
    },
    "./types": {
      "module": "./types/dist/opensaas-keystone-core-types.esm.js",
      "default": "./types/dist/opensaas-keystone-core-types.cjs.js"
    },
    "./fields": {
      "module": "./fields/dist/opensaas-keystone-core-fields.esm.js",
      "default": "./fields/dist/opensaas-keystone-core-fields.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@graphql-ts/extend": "^1.0.0",
    "@graphql-ts/schema": "^0.6.0",
    "@graphql-typed-document-node/core": "^3.1.2",
    "bcryptjs": "^2.4.3",
    "conf": "^10.2.0",
    "dataloader": "^2.1.0",
    "decimal.js": "^10.4.1",
    "graphql": "^16.9.0",
    "inflection": "^3.0.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@opensaas/keystone-core": "workspace:^",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/pluralize": "^0.0.33"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "access.ts",
      "context.ts",
      "lib/index.ts",
      "fields/index.ts",
      "types/index.ts"
    ]
  }
}
